{
  "name": "gimbap",
  "version": "0.0.1",
  "description": "",
  "scripts": {
    "clean": "cross-env rm -rf build/*",
    "build": "cross-env NODE_ENV=production concurrently \"npm run build-client\" \"npm run build-server\" \"npm run build-npm\"",
    "build-client": "webpack",
    "build-server": "tsc -p ./src/server/tsconfig.json",
    "build-npm": "tsc -p ./src/middleware/tsconfig.json",
    "dev": "cross-env NODE_ENV=development concurrently \"webpack serve\" \"nodemon --watch build/server build/server/index.js\" \"npm run watch-build-server\"",
    "watch-build-server": "tsc -p ./src/server/tsconfig.json --watch",
    "watch-build-npm": "tsc -p ./src/middleware/tsconfig.json --watch",
    "start-server": "cross-env NODE_ENV=production node build/server/index.js",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest"
  },
  "jest": {
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oslabs-beta/gimbap.git"
  },
  "keywords": [
    "monolith",
    "microservice",
    "express",
    "typescript",
    "node",
    "react",
    "MUI",
    "gimbap",
    "mongodb",
    "mongoose"
  ],
  "author": "Miguel Hernandez (miguelh72@outlook.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/oslabs-beta/gimbap/issues"
  },
  "homepage": "https://github.com/oslabs-beta/gimbap#readme",
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/plugin-transform-runtime": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@babel/runtime": "^7.15.4",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.2",
    "@types/react": "^17.0.30",
    "@types/react-dom": "^17.0.9",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "babel-jest": "^27.2.5",
    "babel-loader": "^8.2.2",
    "concurrently": "^6.3.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.4.0",
    "css-minimizer-webpack-plugin": "^3.1.1",
    "eslint": "^7.32.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.4.0",
    "image-webpack-loader": "^8.0.1",
    "jest": "^27.2.5",
    "mini-css-extract-plugin": "^2.4.2",
    "nodemon": "^2.0.13",
    "sass": "^1.43.2",
    "sass-loader": "^12.2.0",
    "style-loader": "^3.3.0",
    "supertest": "^6.1.6",
    "typescript": "^4.4.4",
    "webpack": "^5.58.2",
    "webpack-cli": "^4.9.0",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "express": "^4.17.1",
    "mongoose": "^6.0.11",
    "normalize.css": "^8.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
