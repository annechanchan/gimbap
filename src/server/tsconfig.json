{
  "compilerOptions": {
    "outDir": "./../../build",
    
    "module": "commonjs",
    "esModuleInterop": true,
    "target": "es6",
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": ".",
    "sourceMap": true,
    "resolveJsonModule": true,
    "paths": {
      "*": [
        "node_modules/*"
      ]
    }
  },
  "include": [
    "./**/*",
    "../shared/models"
  ]
}
